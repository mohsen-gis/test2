# Install and enable notebook extensions
# This command does not work in BinderHub
#jupyter contrib nbextension install --user
#
#
jupyter nbextension install init_cell

jupyter nbextension enable init_cell/main

jupyter nbextension install python-markdown

jupyter nbextension enable python-markdown/main

# Used for mordecai package
python -m spacy download en_core_web_lg

# Microsoft TrueType core fonts, for the certificate
apt install ttf-mscorefonts-installer

sh setupHourofCI

